{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@assets/*": ["./src/assets/*"],
 
      "@helpers/*": ["./src/helpers/*"],
      "@common/*": ["./src/routes/-common/*"],
      
      "@b2fPortal/*": ["./b2fPortal/*"],
      "@serverB2f/*": ["./b2fPortal/serverB2f/*"],
      "@dummyxPrisma/*": ["./b2fPortal/prisma/dummyx/*"],
      "@dummyxProjectB2f/*": ["./b2fPortal/projectB2f/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
